<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wx" value="200"/>
		<constant name="wy" value="200"/>
		<constant name="wz" value="200"/>
		<constant name="bx" value="10"/>
		<constant name="by" value="10"/>
		<constant name="bz" value="10"/>
	</define>
	<materials>
		<isotope name="G4_H_1" Z="1" N="1">
			<atom value="1.007825"/>
		</isotope>
		<isotope name="G4_H_2" Z="1" N="2">
			<atom value="2.014102"/>
		</isotope>
		<element name="G4_H" formula="H">
			<fraction ref="G4_H_1" n="0.999885"/>
			<fraction ref="G4_H_2" n="0.000115"/>
		</element>
		<material name="G4_Galactic" state="gas">
			<D value="1e-25"/>
			<fraction ref="G4_H" n="1.0"/>
		</material>
		<isotope name="G4_Fe_54" Z="26" N="54">
			<atom value="53.9396"/>
		</isotope>
		<isotope name="G4_Fe_56" Z="26" N="56">
			<atom value="55.9349"/>
		</isotope>
		<isotope name="G4_Fe_57" Z="26" N="57">
			<atom value="56.9354"/>
		</isotope>
		<isotope name="G4_Fe_58" Z="26" N="58">
			<atom value="57.9333"/>
		</isotope>
		<element name="G4_Fe" formula="Fe">
			<fraction ref="G4_Fe_54" n="0.05845"/>
			<fraction ref="G4_Fe_56" n="0.91754"/>
			<fraction ref="G4_Fe_57" n="0.02119"/>
			<fraction ref="G4_Fe_58" n="0.00282"/>
		</element>
		<material name="Material_G4_Fe" state="solid">
			<D value="7.874"/>
			<fraction ref="G4_Fe" n="1.0"/>
		</material>
	</materials>
	<solids>
		<box name="ws" x="wx" y="wy" z="wz" lunit="mm"/>
		<box name="bs" x="bx" y="by" z="bz" lunit="mm"/>
		<scaledSolid name="bsScaled">
			<solidref ref="bs"/>
			<scale x="1" y="2" z="3"/>
		</scaledSolid>
	</solids>
	<structure>
		<volume name="bl">
			<materialref ref="Material_G4_Fe"/>
			<solidref ref="bs"/>
		</volume>
		<volume name="blScaled">
			<materialref ref="Material_G4_Fe"/>
			<solidref ref="bsScaled"/>
		</volume>
		<volume name="wl">
			<materialref ref="G4_Galactic"/>
			<solidref ref="ws"/>
			<physvol name="b_pv1">
				<volumeref ref="bl"/>
			</physvol>
			<physvol name="bscaled_pv1">
				<volumeref ref="blScaled"/>
				<position name="bscaled_pv1_pos" x="0.000000000000000" y="0.000000000000000" z="50.000000000000000" unit="mm"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="wl"/>
	</setup>
</gdml>
